// Fceha de entrega:       Morales Salinas Eduardo 
// Se debe determinar si un numero es positivo o negativo 
// Bibliotecas
#include<stdio.h>
int main() 
{
  float n;
  printf("Ingrese el numero que desea evaluar");
  scanf("%f", &n);
  if(n<0){
  	printf("Tu numero es negativo");
  }
  else{
  	printf("tu numero es positivo");
  }
}
