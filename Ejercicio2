// Se debe de calcular lo que debe de pagar el cliente por un numero de lapices 
// Bibliotecas 
#include<stdio.h>
// Funcion 
int main() {
	int l,p;
	printf("ingrese la cantidad de lapices que va a comprar");
	scanf("%d", l);
	if (l>=1000){
		p=l*85;
		printf("el precio total es de:%i\n", p);
	}
	else {
		p=l*90;
		printf("el precio total es de:%i\n", p);
	}
	
}
